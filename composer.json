{
    "name": "dachcom-digital/dynamic-search",
    "type": "pimcore-bundle",
    "license": "GPL-3.0-or-later",
    "description": "Pimcore >= 6.6 Dynamic Search Bundle",
    "keywords": ["pimcore", "search"],
    "homepage": "https://github.com/dachcom-digital/pimcore-dynamic-search",
    "authors": [
        {
            "name": "DACHCOM.DIGITAL Stefan Hagspiel",
            "email": "shagspiel@dachcom.ch",
            "homepage": "http://www.dachcom.com/",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "DynamicSearchBundle\\": "src/DynamicSearchBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/TestKernel.php"
        ]
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "DynamicSearchBundle\\DynamicSearchBundle"
            ]
        }
    },
    "require": {
        "pimcore/pimcore": "^10.0",
        "laminas/laminas-paginator": "^2.10"
    },
    "require-dev": {
        "codeception/codeception": "~2.4.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "symplify/easy-coding-standard": "^8.0"
    }
}
